#include <bits/stdc++.h>
using namespace std;

int main(){
    
    int steps = 0;
    int f = 0;

    for(int i=0; i<5; i++){
        for(int j=0; j<5; j++){
            int x;
            cin>>x;
            if(x==1){
                steps= abs(j-2) + abs(i-2);
                f=1;
                break;
            }
        }
        if(f==1) break;
    }
    cout<<steps<<endl;
    
    return 0;
}
